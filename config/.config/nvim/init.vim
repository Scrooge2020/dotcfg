set nocompatible
filetype on
filetype plugin on
filetype indent on
syntax on
set number
set shiftwidth=4
set tabstop=4
set nobackup
set scrolloff=10
set nowrap
set ignorecase
set cursorline
set showmatch
set history=50
set title
set mouse=a
set laststatus=0
set noshowcmd
set nofoldenable
set noincsearch
set guicursor=a:
set list
set listchars=tab:\ \  list
vnoremap <C-C> "+y
nnoremap <C-V> "+P
nnoremap <C-C> :noh<CR>
colorscheme noctu
set guicursor=
